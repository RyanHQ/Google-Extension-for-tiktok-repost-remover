{
  "manifest_version": 3,
  "name": "TikTok Repost Remover",
  "version": "1.0",
  "description": "Automatically removes reposted videos on TikTok.",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["https://www.tiktok.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
